/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=aNandb);
    Nand(a=a, b=aNandb, out=aNandaNandb);
    Nand(a=aNandb, b=b, out=aNandbNandb);
    Nand(a=aNandaNandb, b=aNandbNandb, out=out);
}
